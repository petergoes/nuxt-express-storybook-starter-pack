{
  "name": "nuxt-express-storybook-starter-pack",
  "version": "1.0.0",
  "description": "A starter pack for Nuxt with express and storybook",
  "author": "Peter Goes <petergoes@gmail.com>",
  "private": true,
  "scripts": {
    "build": "nuxt build && backpack build",
    "dev": "backpack dev",
    "lint:css": "stylelint '**/*.{vue,css}'",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm-run-all lint:*",
    "lintfix:css": "npm run lint:css -- --fix",
    "lintfix:js": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit":
      "npm run lint && precise-commits --whitelist='**/*.{vue,js,css}'",
    "prepush": "npm run test:ci",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "dependencies": {
    "cross-env": "5.1.4",
    "express": "4.16.3",
    "nuxt": "1.4.0",
    "source-map-support": "0.5.4",
    "universal-fetch": "1.0.0",
    "vue": "2.5.16"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "3.4.1",
    "@storybook/addon-notes": "3.4.1",
    "@storybook/addon-options": "3.4.1",
    "@storybook/addon-storyshots": "3.4.1",
    "@storybook/vue": "3.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "backpack-core": "0.7.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-html": "4.0.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-vue": "4.4.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "jest-vue-preprocessor": "1.4.0",
    "npm-run-all": "4.1.2",
    "postcss": "6.0.21",
    "postcss-html": "0.21.0",
    "postcss-reporter": "5.0.0",
    "precise-commits": "1.0.2",
    "prettier": "1.12.0",
    "regenerator-runtime": "0.11.1",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.4"
  }
}
